{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "dashboard": {
      "type": "object",
      "properties": {
        "links": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        },
        "tunnel": {
          "type": "object",
          "properties": {
            "local_port_min": { "type": "integer" },
            "remote_ports": {
              "type": "array",
              "items": { "type": "integer" },
              "minItems": 1
            }
          },
          "required": ["local_port_min", "remote_ports"],
          "additionalProperties": false
        }
      },
      "required": ["links", "tunnel"],
      "additionalProperties": false
    },
    "commit": {
      "additionalProperties": false,
      "properties": {
        "command": {
          "type": "string"
        },
        "instance": {
          "additionalProperties": false,
          "properties": {
            "accelerator": {
              "type": "string"
            },
            "machine_type": {
              "type": "string"
            },
            "snapshot": {
              "type": "string"
            },
            "zones": {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          },
          "required": [
            "zones",
            "machine_type",
            "snapshot"
          ],
          "type": "object"
        },
        "output_repository": {
          "additionalProperties": false,
          "properties": {
            "branch": {
              "type": "string"
            },
            "deploy_key": {
              "type": "string"
            },
            "url": {
              "format": "uri",
              "type": "string"
            }
          },
          "required": [
            "url",
            "deploy_key"
          ],
          "type": "object"
        },
        "repository": {
          "additionalProperties": false,
          "properties": {
            "branch": {
              "type": "string"
            },
            "deploy_key": {
              "type": "string"
            },
            "url": {
              "format": "uri",
              "type": "string"
            }
          },
          "required": [
            "url",
            "deploy_key"
          ],
          "type": "object"
        }
      },
      "required": [
        "instance",
        "repository",
        "output_repository",
        "command"
      ],
      "type": "object"
    },
    "start": {
      "additionalProperties": false,
      "properties": {
        "command": {
          "type": "string"
        },
        "instance": {
          "additionalProperties": false,
          "properties": {
            "accelerator": {
              "type": "string"
            },
            "machine_type": {
              "type": "string"
            },
            "snapshot": {
              "type": "string"
            },
            "zones": {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          },
          "required": [
            "zones",
            "machine_type",
            "snapshot"
          ],
          "type": "object"
        },
        "repository": {
          "additionalProperties": false,
          "properties": {
            "branch": {
              "type": "string"
            },
            "deploy_key": {
              "type": "string"
            },
            "url": {
              "format": "uri",
              "type": "string"
            }
          },
          "required": [
            "url",
            "deploy_key"
          ],
          "type": "object"
        }
      },
      "required": [
        "instance",
        "repository",
        "command"
      ],
      "type": "object"
    }
  },
  "required": [],
  "type": "object"
}
